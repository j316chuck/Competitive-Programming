    int n, m, u, v, w;
    cin>>n>>m;
    vector< vector<int> > G(n, vector<int> (n, -1));
    for(int i = 0; i < m; i++){
    	cin>>u>>v>>w;
    	u--; v--;
    	if(G[u][v] == -1){
    		G[u][v] = w;
    		G[v][u] = w;
    	}else{
    		G[u][v] = min(G[u][v], w);
    		G[v][u] = min(G[v][u], w);
    	}
    }

